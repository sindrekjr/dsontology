@prefix : <http://www.darksouls.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.darksouls.com/> .

<http://www.darksouls.com/> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.darksouls.com/adjacentTo
:adjacentTo rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Place ;
            rdfs:range :Place .


###  http://www.darksouls.com/appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasVisited .


###  http://www.darksouls.com/belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :ownerOf ;
           rdf:type owl:IrreflexiveProperty ;
           rdfs:domain :Item ;
           rdfs:range :Actor .


###  http://www.darksouls.com/child
:child rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :family ;
       owl:inverseOf :parent ;
       rdf:type owl:AsymmetricProperty ;
       owl:propertyDisjointWith :parent .


###  http://www.darksouls.com/commonSoul
:commonSoul rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :commonality ;
            owl:propertyChainAxiom ( :soul
                                     :belongsTo
                                   ) .


###  http://www.darksouls.com/commonality
:commonality rdf:type owl:ObjectProperty .


###  http://www.darksouls.com/craftedFrom
:craftedFrom rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :obtainedFrom .


###  http://www.darksouls.com/emblem
:emblem rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ,
                 owl:InverseFunctionalProperty ,
                 owl:AsymmetricProperty ,
                 owl:IrreflexiveProperty ;
        rdfs:range :Emblem .


###  http://www.darksouls.com/enemyOf
:enemyOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :knowsOf .


###  http://www.darksouls.com/equipment
:equipment rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :usesItem ;
           rdfs:range :Equipment .


###  http://www.darksouls.com/family
:family rdf:type owl:ObjectProperty ,
                 owl:SymmetricProperty ,
                 owl:TransitiveProperty ;
        rdfs:domain :Character ;
        rdfs:range :Character .


###  http://www.darksouls.com/fights
:fights rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :knowsOf ;
        rdf:type owl:SymmetricProperty .


###  http://www.darksouls.com/friendOf
:friendOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :knowsOf .


###  http://www.darksouls.com/hasItem
:hasItem rdf:type owl:ObjectProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Actor ;
         rdfs:range :Item .


###  http://www.darksouls.com/hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :knowsOf ;
           owl:inverseOf :memberOf ;
           rdfs:domain :Organization ;
           rdfs:range :Character .


###  http://www.darksouls.com/hasVisited
:hasVisited rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :location ;
            rdf:type owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty .


###  http://www.darksouls.com/home
:home rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :hasVisited ;
      rdf:type owl:FunctionalProperty .


###  http://www.darksouls.com/knowsOf
:knowsOf rdf:type owl:ObjectProperty ;
         rdfs:domain :Actor ;
         rdfs:range :Actor .


###  http://www.darksouls.com/leader
:leader rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasMember ;
        owl:inverseOf :leaderOf ;
        rdf:type owl:TransitiveProperty .


###  http://www.darksouls.com/leaderOf
:leaderOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :memberOf .


###  http://www.darksouls.com/location
:location rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:range :Place .


###  http://www.darksouls.com/loyalTo
:loyalTo rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :knowsOf ;
         rdf:type owl:TransitiveProperty .


###  http://www.darksouls.com/memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :loyalTo ;
          rdfs:domain :Character ;
          rdfs:range :Organization .


###  http://www.darksouls.com/mentionedBy
:mentionedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :mentions .


###  http://www.darksouls.com/mentions
:mentions rdf:type owl:ObjectProperty .


###  http://www.darksouls.com/obtainedFrom
:obtainedFrom rdf:type owl:ObjectProperty ,
                       owl:IrreflexiveProperty .


###  http://www.darksouls.com/ownerOf
:ownerOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasItem ;
         rdf:type owl:IrreflexiveProperty .


###  http://www.darksouls.com/parent
:parent rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :family ;
        rdf:type owl:AsymmetricProperty .


###  http://www.darksouls.com/rewardedBy
:rewardedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :obtainedFrom ;
            owl:inverseOf :rewardsItem .


###  http://www.darksouls.com/rewardsItem
:rewardsItem rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasItem .


###  http://www.darksouls.com/sellsItem
:sellsItem rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasItem ;
           owl:inverseOf :soldBy .


###  http://www.darksouls.com/sibling
:sibling rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :family ;
         rdf:type owl:SymmetricProperty ;
         owl:propertyChainAxiom ( :parent
                                  :child
                                ) .


###  http://www.darksouls.com/soldBy
:soldBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :obtainedFrom .


###  http://www.darksouls.com/soul
:soul rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :ownerOf ;
      rdfs:range :Soul .


###  http://www.darksouls.com/speaker
:speaker rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Dialogue ;
         rdfs:range :Character .


###  http://www.darksouls.com/spouse
:spouse rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :family ;
        rdf:type owl:SymmetricProperty .


###  http://www.darksouls.com/tradedFor
:tradedFor rdf:type owl:ObjectProperty ,
                    owl:SymmetricProperty .


###  http://www.darksouls.com/transposedFrom
:transposedFrom rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :obtainedFrom .


###  http://www.darksouls.com/usedBy
:usedBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :belongsTo ;
        owl:inverseOf :usesItem .


###  http://www.darksouls.com/usesItem
:usesItem rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :ownerOf .


###  http://www.darksouls.com/weapon
:weapon rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :usesItem ;
        rdfs:range :Weapon .


#################################################################
#    Data properties
#################################################################

###  http://www.darksouls.com/alias
:alias rdf:type owl:DatatypeProperty .


###  http://www.darksouls.com/description
:description rdf:type owl:DatatypeProperty .


###  http://www.darksouls.com/game
:game rdf:type owl:DatatypeProperty ;
      rdfs:range [ rdf:type rdfs:Datatype ;
                   owl:oneOf [ rdf:type rdf:List ;
                               rdf:first "DS1" ;
                               rdf:rest [ rdf:type rdf:List ;
                                          rdf:first "DS2" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "DS3" ;
                                                     rdf:rest rdf:nil
                                                   ]
                                        ]
                             ]
                 ] .


###  http://www.darksouls.com/gender
:gender rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:oneOf [ rdf:type rdf:List ;
                                 rdf:first "Female" ;
                                 rdf:rest [ rdf:type rdf:List ;
                                            rdf:first "Male" ;
                                            rdf:rest rdf:nil
                                          ]
                               ]
                   ] .


###  http://www.darksouls.com/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :alias ;
      rdf:type owl:FunctionalProperty .


###  http://www.darksouls.com/title
:title rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :alias .


