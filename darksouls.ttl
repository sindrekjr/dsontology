###  http://www.darksouls.com/GwynSoul
:GwynSoul rdf:type owl:NamedIndividual ,
                   :Soul ;
          :mentions :Gwyn ;
          :tradedFor :SunlightSpear ;
          :description """Soul of Gwyn, the Lord of Sunlight
and Cinder, who linked the First Flame.

Lord Gwyn bequeathed most of his power to the
Gods, and burned as cinder for the First
Flame, but even so, Lord Gwyn's soul
is a powerful thing indeed.""" ;
          :game "DS1" ;
          :name "Soul of Gwyn, Lord of Cinder" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :GwynSoul ;
   owl:annotatedProperty :tradedFor ;
   owl:annotatedTarget :SunlightSpear ;
   rdfs:comment "DS1"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :GwynSoul ;
   owl:annotatedProperty :description ;
